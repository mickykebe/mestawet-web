{
  "name": "dallol-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server-watch\" \"npm run client\"",
    "server-watch": "NODE_ENV=development NODE_PATH=dallol-web/src nodemon index.js --exec babel-node",
    "server-watch-prod": "NODE_ENV=production NODE_PATH=dallol-web/src nodemon index.js --exec babel-node",
    "server": "NODE_ENV=development NODE_PATH=dallol-web/src babel-node index.js",
    "client": "cd dallol-web && npm start",
    "test": "NODE_ENV=test nodemon --exec 'mocha --compilers js:babel-register --recursive -R min'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.1",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "glob": "^7.1.2",
    "he": "^1.1.1",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "material-ui": "1.0.0-alpha.17",
    "mocha": "^3.2.0",
    "moment": "^2.18.1",
    "mongoose": "^4.9.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.1",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "serve-favicon": "^2.4.3",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.4.0",
    "supertest": "^3.0.0"
  }
}
